{
    "name": "gubler/ad-search-bundle",
    "type": "symfony-bundle",
    "description": "Active Directory search for Symfony",
    "keywords": [ "activedirectory", "active-directory", "symfony", "bundle" ],
    "homepage": "https://github.com/gubler/gubleradsearchbundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl Gubler",
            "email": "daryl@dev88.co",
            "homepage": "https://dev88.co",
            "role": "Creator"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "gubler/collection": "^1.0",
        "ramsey/uuid": "^4.0",
        "symfony/ldap": "^5.0",
        "symfony/http-kernel": "^^5.0",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "symfony/var-dumper": "^^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Gubler\\ADSearchBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Gubler\\ADSearchBundle\\Test\\": "Tests" }
    }
}
