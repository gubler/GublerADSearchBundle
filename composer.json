{
    "name": "gubler/ad-search-bundle",
    "type": "symfony-bundle",
    "description": "Active Directory search for Symfony",
    "keywords": [ "activedirectory", "active-directory", "search", "users" ],
    "homepage": "https://github.com/gubler/gubleradsearchbundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl Gubler",
            "email": "daryl@dev88.co",
            "homepage": "https://dev88.co"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/serializer": "^3.3|^4.0",
        "illuminate/support": "^5.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0|^4.0",
        "phploc/phploc": "^4.0",
        "pdepend/pdepend": "^2.5",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^2.0",
        "squizlabs/php_codesniffer": "^3.1",
        "matthiasnoback/symfony-config-test": "^2.2",
        "matthiasnoback/symfony-dependency-injection-test": "^1.2",
        "escapestudios/symfony2-coding-standard": "^2.11",
        "phpunit/phpunit": "^6.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Gubler\\ADSearchBundle\\": "" }
    }
}
