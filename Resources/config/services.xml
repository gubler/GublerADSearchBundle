<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="gubler_ad_search.ad_search" class="%gubler_ad_search.ad_search_class%">
            <argument>%gubler_ad_search.ldap_username%</argument>
            <argument>%gubler_ad_search.ldap_password%</argument>
            <argument>%gubler_ad_search.ldap_host%</argument>
            <argument>%gubler_ad_search.ldap_port%</argument>
            <argument>%gubler_ad_search.ldap_base_dn%</argument>
        </service>
    </services>
</container>
