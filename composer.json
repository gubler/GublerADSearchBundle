{
    "name": "gubler/ad-search-bundle",
    "type": "symfony-bundle",
    "description": "Active Directory search for Symfony 3",
    "keywords": [ "activedirectory", "active-directory", "search", "users" ],
    "homepage": "https://github.com/gubler/gubleradsearchbundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daryl Gubler",
            "email": "daryl@dev88.co",
            "homepage": "https://dev88.co"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "symfony/framework-bundle": "3.*",
        "symfony/serializer": "3.*",
        "illuminate/support": "^5.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.0",
        "phploc/phploc": "^2.0.0",
        "pdepend/pdepend": "^2.2.0",
        "phpmd/phpmd": "^2.3.0",
        "sebastian/phpcpd": "^2.0.0",
        "squizlabs/php_codesniffer": "2.*",
        "matthiasnoback/symfony-config-test": "~1.3",
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "escapestudios/symfony2-coding-standard": "~2.0",
        "phpunit/phpunit": "^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Gubler\\ADSearchBundle\\": "" }
    }
}
